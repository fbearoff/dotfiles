alias j jobs
alias jv jobs -v
alias q queue
alias qs queue stop
alias qd queue -d
alias qm queue -m
alias k kill
alias ka kill all
alias p put
alias g get
alias pg pget -n 8
alias m mirror
alias mpg mirror --use-pget-n=8
alias mpl mirror --parallel=5
alias mpgl mirror --use-pget-n=2 --parallel=5
alias mr mirror -r
alias mp mirror -R
alias ll cls -lhtr
alias .. cd .. 
